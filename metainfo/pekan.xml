            <rank>0</rank>
        </state>
        <state name = "run">
            <class>PknExec</class>
            <rank>1</rank>
        </state>
        <state name = "convert">
            <class>PknConvert</class>
            <rank>2</rank>
        </state>
        <state name = "stat">
            <class>PknStats</class>
